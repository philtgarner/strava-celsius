{
	"manifest_version"	: 2,
	"name"				: "__MSG_appName__",
	"description"		: "__MSG_appDescription__",
	"version"			: "0.1",
	"default_locale"	: "en",
	"icons"				: {
		"16"			: "img/logo16.png",
		"48"			: "img/logo48.png",
		"128"			: "img/logo128.png"
	},
	"content_scripts"	: [
		{
			"matches"	: ["https://www.strava.com/activities/*", "http://www.strava.com/activities/*"],
			"js"		: ["jquery-2.1.4.min.js", "jquery-observe.js", "strava_celsius.js"]
		}
	],
	"permissions"		: [
		"activeTab"
	]
}